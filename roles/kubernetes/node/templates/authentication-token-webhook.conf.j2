clusters:
  - name: auth-service
    cluster:
{% if openstack_cacert is defined and openstack_cacert != "" %}
      certificate-authority: "{{ kube_config_dir }}/openstack-cacert.pem"
{% endif %} 
      server: "{{ kube_authentication_token_webhook_url }}"
contexts:
  - context:
      cluster: auth-service
    name: auth-service
current-context: auth-service
